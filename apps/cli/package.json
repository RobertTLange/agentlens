{
  "name": "@roberttlange/agentlens",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "agentlens": "dist/main.js"
  },
  "files": [
    "dist/**",
    "package.json"
  ],
  "bundledDependencies": true,
  "scripts": {
    "dev": "tsx src/main.ts",
    "build": "tsc -b",
    "typecheck": "tsc -b --pretty false",
    "test": "vitest run --passWithNoTests",
    "prepack": "npm --prefix ../.. run build && node scripts/bundle-internal-deps.mjs",
    "postpack": "node scripts/clean-bundle-deps.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@agentlens/contracts": "0.1.0",
    "@agentlens/core": "0.1.0",
    "@agentlens/server": "0.1.0",
    "@fastify/static": "^8.1.0",
    "@iarna/toml": "^2.2.5",
    "chokidar": "^5.0.0",
    "commander": "^13.1.0",
    "fast-glob": "^3.3.3",
    "fastify": "^5.2.1"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
